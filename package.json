    "watch:css": "postcss src/assets/tailwind.css -o src/assets/main.css --watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "tailwindcss": "^1.7.1"
  }
}
